<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>Profile</display-name>
	<welcome-file-list>

		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list> 

	<servlet>
		<servlet-name>login</servlet-name>
		<jsp-file>/login.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login.jsp</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LoginPage</servlet-name>
		<servlet-class>asg.login.LoginPage</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginPage</servlet-name>
		<url-pattern>/LoginPage</url-pattern>
	</servlet-mapping>
	<filter>
		<filter-name>AuthenticationFilter</filter-name>
		<filter-class>asg.login.AuthenticationFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>AuthenticationFilter</filter-name>
		<url-pattern>/LoginPage</url-pattern>
	</filter-mapping>
	<session-config>
		<session-timeout>1</session-timeout>
	</session-config>
	<servlet>
		<servlet-name>profile</servlet-name>
		<jsp-file>/profile.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>profile</servlet-name>
		<url-pattern>/profile</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>EducationalProfile</servlet-name>
		<jsp-file>/EducationalProfile.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>EducationalProfile</servlet-name>
		<url-pattern>/eduProfile</url-pattern>
	</servlet-mapping>
	<filter>
		<filter-name>FormInputFilter</filter-name>
		<filter-class>asg.login.FormInputFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>FormInputFilter</filter-name>
		<url-pattern>/OccupationProfile.jsp</url-pattern>
	</filter-mapping>
	<servlet>
		<servlet-name>OccupationProfile</servlet-name>
		<jsp-file>/OccupationProfile.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>OccupationProfile</servlet-name>
		<url-pattern>/occProfile</url-pattern>
	</servlet-mapping>
	<filter>
		<filter-name>OccupationFilter</filter-name>
		<filter-class>asg.login.OccupationFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>OccupationFilter</filter-name>
		<url-pattern>/ResidentialProfile.jsp</url-pattern>
	</filter-mapping>
	<servlet>
		<servlet-name>ResidentialProfile</servlet-name>
		<jsp-file>/ResidentialProfile.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>ResidentialProfile</servlet-name>
		<url-pattern>/resProfile</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>display</servlet-name>
		<jsp-file>/display.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>display</servlet-name>
		<url-pattern>/show</url-pattern>
	</servlet-mapping>

	<jsp-config>
		<taglib>
			<taglib-uri>www.taherkhalil.com/myTags</taglib-uri>
			<taglib-location>/WEB-INF/log.tld</taglib-location>
		</taglib>
	</jsp-config>
</web-app>